
public class Modulo3Exercicio7 {

	public static void main(String[] args) {
		double nota = 0.0;
		int cont = 0;
		double media = 0.0;
		do{
		nota = Console.readDouble();
		media = media + nota;
		cont += 1;
		}
		while (nota != -1)
		media = media / cont;
		System.out.println("Media: " + media);
		

	}

}
