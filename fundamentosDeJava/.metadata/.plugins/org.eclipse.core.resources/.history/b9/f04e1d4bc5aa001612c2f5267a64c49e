
public class AplicacaoOperacao {

	public static void main(String[] args) {
		Operacao operacao1 = Operacao.SOMA;
		Operacao operacao2 = Operacao.SUBTRACAO;
		Operacao operacao3 = Operacao.MULTIPLICACAO;
		Operacao operacao4 = Operacao.DIVISAO;
		operacao1.calcular(2, 4, 6, Operacao.SOMA);
		operacao2.calcular(2, 4, 6, Operacao.SUBTRACAO);
		operacao3.calcular(2, 4, 6, Operacao.MULTIPLICACAO);
		operacao4.calcular(2, 4, 6, Operacao.DIVISAO);
	}

}
