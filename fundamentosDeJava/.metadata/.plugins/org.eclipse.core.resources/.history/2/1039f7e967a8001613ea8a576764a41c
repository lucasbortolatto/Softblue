public static void main(String[] args){
	String data = ("11/11/2016 03:00");
	
	LocalDateTime d1 = LocalDateTime.parse(data, DateTimeFormatter.ofPattern("dd/MM/yyyy HH:mm"));
	LocalDateTime d2 = LocalDateTime.now();


}
