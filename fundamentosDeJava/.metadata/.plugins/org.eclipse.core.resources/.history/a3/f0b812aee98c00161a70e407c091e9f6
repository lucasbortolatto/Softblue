
public class Modulo6VeiculoTesta {

	public static void main(String[] args) {
		
		Modulo6Veiculo automovel = new Modulo6Automovel();

		automovel.ligar();
		automovel.desligar();
		System.out.println(automovel.isLigado());
		System.out.println(" ");
		
		Modulo6Motocicleta motocicleta = new Modulo6Motocicleta();
		
		motocicleta.ligar();
		motocicleta.desligar();
		System.out.println(motocicleta.isLigado());
		System.out.println(" ");
		
		Modulo6Onibus onibus = new Modulo6Onibus();
		onibus.ligar();
		onibus.desligar();
		System.out.println(onibus.isLigado());
		System.out.println(" ");
		
		
		

	}

}
