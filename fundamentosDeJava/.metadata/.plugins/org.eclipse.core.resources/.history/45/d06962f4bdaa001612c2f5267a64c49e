
public class Matematica {
	double angulo;
	TipoAngulo tipo;
	
	public enum TipoAngulo{
			GRAUS,
			RADIANOS;
	}
	
	Double converterAngulo(Double angulo, TipoAngulo tipo){
		this.angulo = angulo;
		this.tipo = tipo;
		if (tipo == tipo.GRAUS){
			System.out.println(tipo.GRAUS);
		}else{
			System.out.println(tipo.RADIANOS);
		}
			
		return 1.1;
		
	}

}
