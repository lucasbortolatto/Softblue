
public class Modulo3Exercicio7 {

	public static void main(String[] args) {
		double nota = 1;
		int cont = 0;
		double media = 1.0;
		do{
		nota = Console.readDouble();
		cont =+ 1;
		media = media + nota;
		}while (nota != -1);
		media = media / cont;
		System.out.println("Media: " + media);
		

	}

}
