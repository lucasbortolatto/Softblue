
public class SaldoInsuficienteException extends Exception {
	double valor;
	double saldo;

	public SaldoInsuficienteException(double valor) {
		this.valor = valor;
		System.out.println("O saldo é de " + saldo +  " , Saldo insuficiente para o saque de " + valor);
	}

	public double getValor() {
		return valor;
	}
	
	
	
	
}
