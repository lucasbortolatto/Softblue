
public class Modulo6Veiculo {
	private boolean ligado;
	
	public void ligar(){
		this.ligado = true;
		System.out.println("Automovel Ligado");
	}
	
	public void desligar(){
		this.ligado = false;
		System.out.println("Automovel Desligado");
	}

	public boolean isLigado() {
		return this.ligado;
	}
	

}
